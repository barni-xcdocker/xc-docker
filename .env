COMPOSE_PROJECT_NAME=xc4

TLD_DOMAIN=.loc

TAG_PHP_FPM=xc4.0
TAG_WORKSPACE=xc4.0

# Point to the relative path of your applications code on your host
APP_CODE_PATH_HOST=../

# Point to where the `APP_CODE_PATH_HOST` should be in the container
# Make sure no trailing space appear
APP_CODE_PATH_CONTAINER=/var/www

# You may add flags to the path `:cached`, `:delegated`. When using Docker Sync add `:nocopy`
APP_CODE_CONTAINER_FLAG=:cached

WEB_USER=www-data
WEB_GROUP=www-data

APACHE_HOST_HTTP_PORT=80
APACHE_HOST_HTTPS_PORT=443
APACHE_ROOT_DIR=/usr/local/apache2

MYSQL_CONTAINER_USER=mysql
MYSQL_CONTAINER_GROUP=mysql
MYSQL_ROOT_PASSWORD=pass
